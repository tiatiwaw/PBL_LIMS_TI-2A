    "notes": {
        "supervisor_flow": [
            "1. GET /api/v1/supervisor/orders - List semua order",
            "2. GET /api/v1/supervisor/orders/1 - Detail order",
            "3. PUT /api/v1/supervisor/orders/1 - Approve/Reject order",
            "4. GET /api/v1/supervisor/orders/1/parameters - List parameters",
            "5. POST /api/v1/supervisor/orders/1/parameters/submit - Submit parameters & assign analyst"
        ],
        "staff_flow": [
            "1. POST /api/v1/staff/orders - Create new order",
            "2. POST /api/v1/staff/orders/samples - Add sample ke order",
            "3. PUT /api/v1/staff/orders/1 - Update order info",
            "4. DELETE /api/v1/staff/orders/1 - Delete order (sebelum supervisor approve)"
        ],
        "staff_client_management_flow": [
            "1. GET /api/v1/staff/manage-clients - List semua clients",
            "2. GET /api/v1/staff/manage-clients/1 - Detail client",
            "3. POST /api/v1/staff/manage-clients - Create new client",
            "4. PUT /api/v1/staff/manage-clients/1 - Update client data",
            "5. DELETE /api/v1/staff/manage-clients/1 - Delete client"
        ],
        "order_types": [
            "regular - Order normal",
            "urgent - Order urgent/prioritas",
            "internal - Order internal perusahaan",
            "external - Order dari klien eksternal"
        ]
    }
}
